{
  "env": {
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "jest"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "arrow-parens": [
      0,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "import/prefer-default-export": 0,
    "import/no-cycle": 0
  }
}
