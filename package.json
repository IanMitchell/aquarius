{
  "name": "aquarius",
  "version": "2.0.0",
  "description": "A highly configurable Discord chatbot",
  "main": "index.js",
  "repository": "ianmitchell/aquarius",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": {
    "name": "Ian Mitchell",
    "email": "ian.mitchel1@live.com",
    "url": "http://ianmitchell.io"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=13.5.0"
  },
  "scripts": {
    "start": "cross-env DEBUG=*,-engine*,-babel*,-send,-ref*,-express* node -r dotenv/config --es-module-specifier-resolution=node index.js",
    "build": "node scripts/install-keyfile",
    "build:workspaces": "yarn workspaces run build",
    "debug": "cross-env DEBUG=*,-socket.io*,-engine*,-babel*,-send,-ref* node --nolazy --inspect-brk=9229 -r dotenv/config --es-module-specifier-resolution=node index.js",
    "prelint": "yarn run build:workspaces",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "pretest": "yarn run build:workspaces",
    "test": "jest"
  },
  "dependencies": {
    "@aquarius/regex": "^1.0.0",
    "@google-cloud/firestore": "^3.7.2",
    "@sentry/node": "5.15.4",
    "alphavantage": "^2.0.0",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.11.1",
    "debug": "^4.1.1",
    "dedent-js": "^1.0.1",
    "discord.js": "^11.5.0",
    "discord.js-minesweeper": "^1.0.5",
    "dotenv": "^8.2.0",
    "downsize": "^0.0.8",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "js-yaml": "^3.10.0",
    "node-fetch": "^2.3.0",
    "node-vibrant": "^3.1.5",
    "parse-color": "^1.0.0",
    "pluralize": "^8.0.0",
    "pretty-bytes": "^5.0.0",
    "rgb-hex": "^3.0.0",
    "rss-parser": "^3.7.6",
    "timber": "^3.1.3",
    "turndown": "^6.0.0",
    "unzalgo": "^2.1.2",
    "xmldom": "^0.3.0",
    "yahoo-stocks": "^2.0.4"
  },
  "devDependencies": {
    "@aquarius/testing": "^0.0.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "jest-discord": "^0.1.0",
    "jest-environment-discord": "^0.1.0",
    "jest-github-actions-reporter": "^1.0.2",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.5"
  }
}
