    "@aquarius/permissions": "file:packages/permissions",
    "@aquarius/progress-bar": "file:packages/progress-bar",
    "@aquarius/regex": "file:packages/regex",
    "@aquarius/sentry": "file:packages/sentry",
    "@aquarius/testing": "file:packages/testing",
    "@aquarius/triggers": "file:packages/triggers",
    "@aquarius/users": "file:packages/users",
    "aquarius": "file:packages/aquarius",
    "aquarius-dashboard": "file:packages/dashboard"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@sentry/node": "5.15.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.3.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "discord.js": "^12.2.0",
    "jest-discord": "file:packages/jest-discord",
    "jest-discord-fakes": "file:packages/jest-discord-fakes",
    "jest-environment-discord": "file:packages/jest-environment-discord",
    "jest-discord-bot": "file:packages/jest-discord-bot",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.3.0",
    "jest-github-actions-reporter": "^1.0.2",
    "lerna": "^3.20.2",
    "pectin": "3.6.1",
    "prettier": "^2.0.4",
    "regenerator-runtime": "^0.13.5"
