{
	"name": "aquarius",
	"version": "3.0.0",
	"description": "A highly configurable Discord chatbot",
	"main": "index.js",
	"repository": "ianmitchell/aquarius",
	"author": {
		"name": "Ian Mitchell",
		"email": "ian.mitchell@hey.com",
		"url": "http://ianmitchell.dev"
	},
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=18.9.0"
	},
	"scripts": {
		"start": "cross-env DEBUG_COLORS=true DEBUG=*,-engine*,-babel*,-send,-ref*,-express*,-prisma:*,-avvio,-plusX,-agentkeepalive,-follow-redirects node -r dotenv/config --es-module-specifier-resolution=node index.js",
		"debug": "cross-env DEBUG_COLORS=true DEBUG=*,-socket.io*,-engine*,-babel*,-send,-ref* node --nolazy --inspect -r dotenv/config --es-module-specifier-resolution=node index.js --color",
		"lint": "eslint . --ext .js --ignore-path .gitignore",
		"prettier": "prettier **/*.js --check",
		"db:create": "npx prisma migrate deploy && npx prisma generate",
		"db:migrate": "npx prisma migrate dev && npx prisma generate"
	},
	"dependencies": {
		"@discordjs/builders": "^1.2.0",
		"@fastify/cors": "^8.1.0",
		"@prisma/client": "^4.3.1",
		"@sentry/node": "^7.13.0",
		"chalk": "^5.0.1",
		"cross-env": "^7.0.0",
		"date-fns": "^2.29.3",
		"debug": "^4.1.1",
		"dedent-js": "^1.0.1",
		"discord.js": "^14.5.0",
		"dotenv": "^16.0.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-xo": "^0.42.0",
		"eslint-config-xo-typescript": "^0.53.0",
		"eslint-import-resolver-typescript": "^3.5.1",
		"eslint-plugin-prettier": "^4.2.1",
		"fastify": "^4.6.0",
		"leven": "^4.0.0",
		"node-cache": "5.1.2",
		"node-vibrant": "^3.2.1-alpha.1",
		"pluralize": "^8.0.0",
		"pretty-bytes": "^6.0.0",
		"rgb-hex": "^4.0.0",
		"rss-parser": "^3.7.3",
		"supports-color": "^9.2.3",
		"undici": "^5.11.0"
	},
	"devDependencies": {
		"@types/debug": "^4.1.7",
		"@types/node": "^18.7.3",
		"@types/pluralize": "^0.0.29",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"eslint": "^8.24.0",
		"eslint-plugin-import": "^2.26.0",
		"prettier": "^2.7.1",
		"prisma": "^4.2.0",
		"typescript": "^4.7.4"
	},
	"volta": {
		"node": "18.9.1"
	}
}
